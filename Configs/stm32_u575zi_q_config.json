{
    "inherits": "stm32_config.json",
    
    "vendor":   "stm32",
    "board":    "STM32U575",
    "family":   "stm32u5xx",

    
    "build":{
        
        "alayer":               "stm32u5xx-hal-driver",
        "startup_script":       "startup_stm32u575xx.s",
        "linker_script":        "STM32U575ZITXQ_FLASH.ld",
        "startup_script_path":  "cmsis-device-u5/Source/Templates/gcc",

        "includes":{
            "cmsis_compiler":   "CMSIS_5/CMSIS/Core/Include/cmsis_compiler.h",
            "cmsis_gcc":        "CMSIS_5/CMSIS/Core/Include/cmsis_gcc.h",
            "cmsis_version":    "CMSIS_5/CMSIS/Core/Include/cmsis_version.h",
            "core_cm":          "CMSIS_5/CMSIS/Core/Include/core_cm33.h",
            "mpu_arm":          "CMSIS_5/CMSIS/Core/Include/mpu_armv8.h",
            "mbl":              "CMSIS_5/CMSIS/Core/Include/core_armv8mbl.h",
            "mml":              "CMSIS_5/CMSIS/Core/Include/core_armv8mml.h",
            "1mml":             "CMSIS_5/CMSIS/Core/Include/core_armv81mml.h",
            "pac":              "CMSIS_5/CMSIS/Core/Include/pac_armv81.h",
            "pmu":              "CMSIS_5/CMSIS/Core/Include/pmu_armv8.h",
            "tz":               "CMSIS_5/CMSIS/Core/Include/tz_context.h",

            "family":           "cmsis-device-u5/Include/stm32u5xx.h",
            "board":            "cmsis-device-u5/Include/stm32u575xx.h",
            "system":           "cmsis-device-u5/Include/system_stm32u5xx.h",
            "partition":        "cmsis-device-u5/Include/partition_stm32u5xx.h"

        }
    },

    "openocd":{
        "interface":        "stlink",
        "target":           "stm32u5x"
    },


    "uart": {
        "instance": "USART1",
        "baudrate": 115200,
        "tx_pin": "PA9",
        "rx_pin": "PA10"
    },

    "timers": {
        "tim1": {
            "period": 65535,
            "trigger": ["tim5", "tim2", "tim3", "tim4"]
        },
        "tim8": {
            "period": 65535,
            "trigger": ["tim1", "tim2", "tim4", "tim5"]
        },
        "tim2": {
            "period": 4294967295,
            "trigger": ["tim1", "tim8", "tim3", "tim4"]
        },
        "tim3": {
            "period": 65535,
            "trigger": ["tim1", "tim2", "tim15", "tim4"]
        },
        "tim4": {
            "period": 65535,
            "trigger": ["tim1", "tim2", "tim3", "tim8"]
        },
        "tim5": {
            "period": 4294967295,
            "trigger": ["tim2", "tim3", "tim4", "tim8"]
        }

        
        ,
        "tim15": {
            "period": 65535,
            "trigger": ["tim1", "tim3", "tim16_OC1", "tim17_OC1"]
        }


    }
}