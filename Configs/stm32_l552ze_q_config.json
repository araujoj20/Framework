{
    "inherits": "stm32_config.json",
    "board": "STM32L552",
    "family": "stm32l5xx",
    "uart": {
        "instance": "LPUART1",
        "baudrate": 115200,
        "tx_pin": "PG7",
        "rx_pin": "PG8"
    },

    "timers": {
        "tim1": {
            "period": 65535,
            "trigger": ["tim15", "tim2", "tim3", "tim4"]
        },
        "tim8": {
            "period": 65535,
            "trigger": ["tim1", "tim2", "tim4", "tim5"]
        },
        "tim2": {
            "period": 4294967295,
            "trigger": ["tim1", "tim8", "tim3", "tim4"]
        },
        "tim3": {
            "period": 65535,
            "trigger": ["tim1", "tim2", "tim15", "tim4"]
        },
        "tim4": {
            "period": 65535,
            "trigger": ["tim1", "tim2", "tim3", "tim8"]
        },
        "tim5": {
            "period": 4294967295,
            "trigger": ["tim2", "tim3", "tim4", "tim8"]
        }

        
        ,
        "tim15": {
            "period": 65535,
            "trigger": ["tim1", "tim3", "tim16_OC1", "tim17_OC1"]
        }

    }
}